./104intersection 1 0 0 2 1 1 0 1
echo "Delta < 0"  && echo "-------------------------------------------------------------"
./104intersection 1 4 0 3 0 0 -2 4
echo "Delta = 0" && echo "-------------------------------------------------------------"
./104intersection 2 0 0 2 1 1 0 1
echo "Delta > 0" && echo "-------------------------------------------------------------"
./104intersection 3 -1 -1 -1 1 1 5 30
echo "Delta > 0" && echo "-------------------------------------------------------------"
./104intersection 2 1 0 0 0 0 1 1
echo "Delta = 0 but infinite solutions" && echo "-------------------------------------------------------------"